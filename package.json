{
  "name": "spred",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverage --coverageDirectory=\"./coverage\"",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "test:performance": "jest --testPathPattern=__tests__ --testNamePattern=performance --verbose",
    "test:integration": "jest --testPathPattern=__tests__ --testNamePattern=integration --verbose",
    "test:e2e": "jest --testPathPattern=__tests__ --testNamePattern=e2e --verbose",
    "test:all": "npm run test:ci && npm run test:performance && npm run test:integration",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ..",
    "postinstall": "patch-package",
    "quality:check": "npm run type-check && npm run lint && npm run test:ci",
    "quality:fix": "npm run lint:fix && npm run type-check",
    "performance:monitor": "node scripts/performance-monitor.js",
    "performance:benchmark": "jest --testPathPattern=__tests__ --testNamePattern=benchmark --verbose",
    "monitor:start": "node scripts/realtime-monitor.js",
    "monitor:dev": "node scripts/realtime-monitor.js --dev",
    "monitor:prod": "node scripts/realtime-monitor.js --prod",
    "monitor:install": "npm install ws",
    "monitor:test": "node scripts/realtime-monitor.js --test",
    "monitor:status": "curl -s http://localhost:8080/status || echo 'Monitoring server not running'"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.5.0",
    "crypto-js": "3.1.9-1",
    "i18next": "^22.4.10",
    "p2p-file-transfer": "file:.yalc/p2p-file-transfer",
    "react": "18.2.0",
    "react-i18next": "^12.2.0",
    "react-native": "0.73.5",
    "react-native-aes-crypto": "^2.1.1",
    "react-native-blurhash": "^2.1.2",
    "react-native-camera": "^4.2.1",
    "react-native-create-thumbnail": "^2.0.0",
    "react-native-document-picker": "^9.3.1",
    "react-native-file-viewer": "^2.1.5",
    "react-native-fs": "^2.20.0",
    "react-native-image-picker": "^7.1.0",
    "react-native-mmkv": "^2.12.1",
    "react-native-modalize": "^2.1.1",
    "react-native-multipeer-connectivity": "^0.1.0",
    "react-native-orientation-locker": "^1.7.0",
    "react-native-pager-view": "^6.9.1",
    "react-native-paper": "^5.12.3",
    "react-native-permissions": "^5.4.2",
    "react-native-progress": "^5.0.1",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.27.0",
    "react-native-share": "^12.2.0",
    "react-native-snackbar": "^2.6.2",
    "react-native-svg": "^14.2.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-video": "^5.2.1",
    "react-native-webview": "^13.3.1",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "text-encoding-polyfill": "^0.6.7",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "^0.73.5",
    "@react-navigation/devtools": "^6.0.18",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.14.1",
    "@types/react": "^18.2.6",
    "@types/react-native-video": "^5.0.15",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "patch-package": "^8.0.1",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
